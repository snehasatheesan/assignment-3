1.Take three numbers from the user and print the greatest number.

import java.util.Scanner;

public class prob1 {
    public static void main(String[] args) {
Scanner in = new Scanner(System.in);
     int a= in.nextInt();
        int b=in.nextInt();
      int c=in.nextInt();
         int max=a;

        if(b>max){
          max=b;
        }
if
    (c>max){
        max=c;
    }
 else{
            max=a;
 }
        System.out.println(max);
        in.close();
}
}
